# Mailchimp API Configuration
MAILCHIMP_API_KEY=your_full_api_key_here-us9
MAILCHIMP_SERVER_PREFIX=us9

# Server Configuration
MAILCHIMP_READONLY=true
MAILCHIMP_MASK_PII=true
TRANSPORT_MODE=stdio

# HTTP Mode Configuration (only if using TRANSPORT_MODE=http)
# Comma-separated list of allowed CORS origins (empty = localhost only)
# Example: ALLOWED_ORIGINS=https://example.com,https://app.example.com
# ALLOWED_ORIGINS=

# Optional: Required only if you want to send campaigns
# CONFIRM_SEND=I_KNOW_WHAT_IM_DOING
